<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="css/style.css">
    <link rel="stylesheet" href="css/styletabla.css">
    <%- include('./fijos/menu.ejs', {variant:'compact'}) %>
      
              <div class="main-content">
                <div class="header">
                  <div class="page-display">
                    <h1>CUENTAS DE ESTUDIANTES</h1>
                  </div>
                </div>
      
                <hr>
                <div class="featured-clay">

                            <table class="responstable">
                            
                                <thead>
                                    <tr>
                                      <th>ID</th>
                                      <th>Nombre</th>
                                      <th>Email</th>
                                    </tr>
                                  </thead>
                            <tbody>
                                <% users.forEach((user) => { %>
                                    <tr>
                                      <td><%= user.id %></td>
                                      <td><%= user.nombre %></td>
                                      <td><%= user.email %></td>
                                    </tr>
                                  <% }) %>
                              </tbody>
                            
                            
                            </table>
                </div>
                <hr>
              </div>
      
            </div>
      
          </div>
      
        </div>
      
      </div>
</body>
</html>